openapi: 3.0.0
info:
  title: Openbanking.co.uk Account and Transaction API v3.1.0
  version: 1.0.0
  description: >-
    version=3.1.0 - Openbanking.co.uk for Account and Transaction API
    Specification as decribed on
    https://raw.githubusercontent.com/OpenBankingUK/read-write-api-specs/v3.1.0/dist/account-info-swagger.yaml
servers:
  - url: https:://open-banking
  - url: '{{host}}'
paths:
  /v3.1/aisp/account-access-consents:
    post:
      summary: Create Account Access Consents
      tags:
        - account-access-consents
      parameters:
        - name: x-fapi-financial-id
          in: header
          schema:
            type: string
        - name: x-fapi-customer-last-logged-time
          in: header
          schema:
            type: string
        - name: x-fapi-customer-ip-address
          in: header
          schema:
            type: string
        - name: x-fapi-interaction-id
          in: header
          schema:
            type: string
        - name: Authorization
          in: header
          schema:
            type: string
        - name: x-customer-user-agent
          in: header
          schema:
            type: string
      responses:
        '200':
          description: Setup Account Access Consents - Limited Permissions
          headers:
            x-fapi-interaction-id:
              schema:
                type: string
          content:
            application/json:
              schema:
                type: object
                properties:
                  Data:
                    type: object
                    properties:
                      ConsentId:
                        type: string
                      Status:
                        type: string
                      StatusUpdateDateTime:
                        type: string
                        format: date-time
                      CreationDateTime:
                        type: string
                        format: date-time
                      Permissions:
                        type: array
                        items:
                          type: string
                      ExpirationDateTime:
                        type: string
                        format: date-time
                      TransactionFromDateTime:
                        type: string
                        format: date-time
                      TransactionToDateTime:
                        type: string
                        format: date-time
                  Risk:
                    type: object
                  Links:
                    type: object
                    properties:
                      Self:
                        type: string
                        format: uri
                  Meta:
                    type: object
                    properties:
                      TotalPages:
                        type: integer
        '201':
          description: Setup Account Access Consents - All Permission Granted
          headers:
            x-fapi-interation-id:
              schema:
                type: string
          content:
            application/json:
              schema:
                type: object
                properties:
                  Data:
                    type: object
                    properties:
                      ConsentId:
                        type: string
                      Status:
                        type: string
                      StatusUpdateDateTime:
                        type: string
                        format: date-time
                      CreationDateTime:
                        type: string
                        format: date-time
                      Permissions:
                        type: array
                        items:
                          type: string
                      ExpirationDateTime:
                        type: string
                        format: date-time
                      TransactionFromDateTime:
                        type: string
                        format: date-time
                      TransactionToDateTime:
                        type: string
                        format: date-time
                  Risk:
                    type: object
                  Links:
                    type: object
                    properties:
                      Self:
                        type: string
                        format: uri
                  Meta:
                    type: object
                    properties:
                      TotalPages:
                        type: integer
