          schema:
            type: string
        - name: x-fapi-customer-last-logged-time
          in: header
          schema:
            type: string
        - name: x-fapi-customer-ip-address
          in: header
          schema:
            type: string
        - name: x-fapi-interaction-id
          in: header
          schema:
            type: string
        - name: Authorization
          in: header
          schema:
            type: string
        - name: x-customer-user-agent
          in: header
          schema:
            type: string
      responses:
        '204':
          description: Delete Account Access Consents
          headers:
            x-fapi-interaction-id:
              schema:
                type: string
